{
  "name": "sparrow-ai-tech",
  "version": "0.2.1",
  "description": "Modern Astro + React web project with AI tools, infographics, and interactive features",
  "private": "true",
  "type": "module",
  "homepage": "https://sparesparrow.github.io/sparrow-ai-tech/",
  "repository": {
    "type": "git",
    "url": "https://github.com/sparesparrow/sparrow-ai-tech.git"
  },
  "author": "sparesparrow <sparesparrow@protonmail.ch>",
  "license": "MIT",
  "keywords": "[\"astro\", \"react\", \"tailwindcss\", \"ai-tools\", \"infographics\", \"mermaid\", \"chartjs\", \"github-pages\", \"static-site\"]",
  "scripts": {
    "dev": "astro dev --host --port 3000",
    "start": "astro dev --host",
    "build": "astro check && astro build",
    "preview": "astro preview --host --port 4321",
    "astro": "astro",
    "lint": "eslint src",
    "test": "echo 'No tests configured yet'",
    "check": "npm run lint && npm test",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "test:e2e:ci": "cypress run --browser chrome --headless",
    "clean": "rm -rf dist .astro node_modules/.cache cypress/screenshots cypress/videos",
    "clean:install": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "deploy": "npm run build && gh-pages -d dist",
    "fix-urls": "node scripts/fix-urls.cjs",
    "verify": "node scripts/verify-deployment.js",
    "validate": "node scripts/validate-urls.cjs",
    "deps:check": "npm outdated",
    "deps:audit": "npm audit --audit-level moderate"
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@astrojs/svelte": "^7.1.0",
    "@astrojs/tailwind": "^5.1.5",
    "astro": "^5.12.6",
    "chart.js": "^4.5.0",
    "framer-motion": "^12.23.12",
    "mermaid": "^11.9.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.7.1",
    "react-tooltip": "^5.29.1",
    "typescript": "^5.8.3",
    "yet-another-react-lightbox": "^3.25.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@babel/eslint-parser": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/preset-react": "^7.23.0",
    "@tailwindcss/typography": "^0.5.16",
    "@types/prop-types": "^15.7.15",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "autoprefixer": "^10.4.21",
    "browserslist": "^4.25.1",
    "cypress": "^13.17.0",
    "eslint": "^8.57.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.3.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.17"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "bugs": {
    "url": "https://github.com/sparesparrow/sparrow-ai-tech/issues"
  }
}
