{
  "name": "sparrow-ai-tech",
  "version": "0.1.8",
  "description": "Modern Astro + React web project with i18n, Tailwind CSS, and GitHub Pages deployment",
  "private": true,
  "type": "module",
  "homepage": "https://sparesparrow.github.io/sparrow-ai-tech/",
  "repository": {
    "type": "git",
    "url": "https://github.com/sparesparrow/sparrow-ai-tech.git"
  },
  "author": "sparesparrow",
  "license": "MIT",
  "keywords": [
    "astro",
    "react",
    "tailwindcss",
    "typescript",
    "static-site",
    "github-pages",
    "i18n"
  ],
  "scripts": {
    "dev": "astro dev --host",
    "start": "astro dev --host",
    "build": "astro check && astro build",
    "build:dev": "astro build --mode development",
    "preview": "astro preview --host",
    "astro": "astro",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --ignore-path .gitignore",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --ignore-path .gitignore --fix",
    "lint:report": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --ignore-path .gitignore --output-file eslint-report.json --format json",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,astro,md,json,css,scss}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,astro,md,json,css,scss}\"",
    "type-check": "astro check",
    "type-check:watch": "astro check --watch",
    "clean": "rm -rf dist .astro node_modules/.cache coverage cypress/screenshots cypress/videos",
    "clean:install": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "fix-all": "npm run lint:fix && npm run format",
    "check-all": "npm run type-check && npm run lint && npm run format:check && npm run test",
    "prepare": "husky install",
    "precommit": "lint-staged",
    "postinstall": "npm run prepare",
    "deps:check": "npm outdated",
    "deps:update": "npm update"
  },
  "dependencies": {
    "@astrojs/react": "^3.6.2",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "astro": "^5.0.3",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/tailwind": "^5.1.1",
    "@eslint/js": "^9.15.0",
    "@tailwindcss/typography": "^0.5.15",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.1",
    "@testing-library/user-event": "^14.5.2",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "astro-eslint-parser": "^1.0.3",
    "cypress": "^13.17.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-astro": "^1.2.4",
    "eslint-plugin-cypress": "^3.5.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "globals": "^15.12.0",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "prettier-plugin-astro": "^0.14.1",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.38.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,astro}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,json,css,scss}": [
      "prettier --write"
    ]
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": [
      "<rootDir>/jest.setup.js"
    ],
    "testMatch": [
      "**/__tests__/**/*.(js|jsx|ts|tsx)",
      "**/*.(test|spec).(js|jsx|ts|tsx)"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "collectCoverageFrom": [
      "src/**/*.(js|jsx|ts|tsx)",
      "!src/**/*.d.ts"
    ]
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
