{
  "name": "sparrow-ai-tech",
  "version": "0.2.1",
  "description": "Modern Astro + React web project with AI tools, infographics, and interactive features",
  "private": "true",
  "type": "module",
  "homepage": "https://sparesparrow.github.io/sparrow-ai-tech/",
  "repository": {
    "type": "git",
    "url": "https://github.com/sparesparrow/sparrow-ai-tech.git"
  },
  "author": "sparesparrow <sparesparrow@protonmail.ch>",
  "license": "MIT",
  "keywords": "[\"astro\", \"react\", \"tailwindcss\", \"ai-tools\", \"infographics\", \"mermaid\", \"chartjs\", \"github-pages\", \"static-site\"]",
  "scripts": {
    "astro": "astro",
    "build": "NODE_OPTIONS=--max-old-space-size=4096 astro build",
    "build:check": "NODE_OPTIONS=--max-old-space-size=4096 astro check && astro build",
    "check": "astro check",
    "clean": "rm -rf dist .astro node_modules/.cache cypress/screenshots cypress/videos",
    "clean:install": "npm run clean && rm -rf node_modules package-lock.json && npm install",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "deps:check": "npm outdated",
    "deps:audit": "npm audit --audit-level moderate",
    "deploy": "npm run build && npx gh-pages -d dist",
    "dev": "astro dev",
    "dev:host": "astro dev --host --port 3000",
    "fix-urls": "node scripts/fix-urls.cjs",
    "preview": "astro preview",
    "preview:host": "astro preview --host --port 4321",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "start": "astro dev",
    "start:host": "astro dev --host",
    "test": "echo 'No tests configured yet'",
    "test:jest": "jest",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:e2e:headless": "cypress run --headless",
    "test:e2e:ci": "cypress run --browser chrome --headless",
    "validate": "node scripts/validate-urls.cjs",
    "verify": "node scripts/verify-deployment.js"
  },
  "dependencies": {
    "@astrojs/react": "^4.3.0",
    "@astrojs/tailwind": "^5.1.5",
    "@tippyjs/react": "^4.2.6",
    "astro": "^5.12.6",
    "chart.js": "^4.5.0",
    "framer-motion": "^12.23.12",
    "marked": "^4.0.0",
    "mermaid": "^11.9.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.3.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.7.1",
    "react-tooltip": "^5.29.1",
    "tailwindcss": "^3.4.17",
    "tippy.js": "^6.3.7",
    "typescript": "^5.8.3",
    "yet-another-react-lightbox": "^3.25.0"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/svelte": "^7.1.0",
    "@babel/core": "^7.28.0",
    "@babel/eslint-parser": "^7.28.0",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@tailwindcss/typography": "^0.5.16",
    "@types/prop-types": "^15.7.15",
    "@types/react-helmet": "^6.1.11",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "autoprefixer": "^10.4.21",
    "babel-jest": "^29.7.0",
    "browserslist": "^4.25.1",
    "chart": "^0.1.2",
    "cypress": "^13.17.0",
    "cypress-axe": "^1.6.0",
    "eslint": "^9.32.0",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "gh-pages": "^6.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "postcss": "^8.5.6",
    "prettier": "^3.0.0",
    "prettier-plugin-astro": "^0.12.3",
    "prettier-plugin-tailwindcss": "^0.5.14",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "bugs": {
    "url": "https://github.com/sparesparrow/sparrow-ai-tech/issues"
  }
}
