{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@pages/*": [
        "./src/pages/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ],
      "@languages/*": [
        "./src/languages/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}