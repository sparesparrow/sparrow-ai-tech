{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "allowJs": true,
    "checkJs": false,
    "skipLibCheck": true,
    "noEmit": true,
    "types": ["cypress", "node", "jest"]
  },
  "include": ["cypress/**/*.js", "src/**/*.js", "src/**/*.jsx"],
  "exclude": ["node_modules", "dist"]
}
